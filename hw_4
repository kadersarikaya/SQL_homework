-- query 1
SELECT DISTINCT replacement_cost FROM film;

-- query 2
SELECT COUNT(DISTINCT replacement_cost) FROM film;

-- query 3
SELECT COUNT(*) FROM film 
WHERE title LIKE 'T%' 
AND rating = 'G';

-- query 4
SELECT COUNT(*) FROM country
WHERE country LIKE '_____';

-- query 5
select count(*) FROM city
WHERE city ILIKE '%r'
